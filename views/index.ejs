<!-- ================
        HEADING
     ================ -->

<section id="content">
  <h1>Welcome to Git Racer</h1>
</section>


<!-- ================
        MAIN IMAGE
     ================ -->

  <section id="octocat">
    <img src="/images/cat2.png">
  </section>


<!-- =====================
          SIGN IN FORM
     ===================== -->


<form class="sign-in">
  <input id="githubUsername" name="username" type="text" class="username_input" placeholder="GitHub Username..." />
  <a href='#' id="sign-in-button">Race!!!</a>
  <p class="error"></p>
</form>


<!-- ================
        BAR FILLER

     ================ -->
<section class="bars">
</section>

<template id="template_meter">
  <h2 id="username_{{username}}"></h2>
  <h3 id="commits_{{username}}"> </h3>
  <meter id='meter_{{username}}' value="0" min="0" max="25"><br /></meter>
</template>



